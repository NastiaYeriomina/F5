# http://EditorConfig.org
root = true

[*]
indent_style = space
indent_size = 3
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8

[*.js]
indent_size = 3

[*.scss]
indent_size = 3

[{bower.json,package.json,.travis.yml}]
indent_style = space
indent_size = 2
